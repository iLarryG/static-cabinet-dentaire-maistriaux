<div id="map" style="width: 100%; height: 400px;"></div>

      <script type="text/javascript" src="https://maps.google.com/maps/api/js?libraries=places&key=AIzaSyCqNSZmuiLIzhMBq3Tpn7TnexS70hLeNjs"></script>

      <script type="text/javascript">
        var myLatlng = new google.maps.LatLng(50.726549, 5.742300);

        var myOptions = {
          zoom: 10,
          center: myLatlng,
          scrollwheel: false,
          mapTypeId: google.maps.MapTypeId.ROADMAP,
  styles: [{"stylers":[{"hue":"#dd0d0d"}]},{"featureType":"road","elementType":"labels","stylers":[{"visibility":"off"}]},{"featureType":"road","elementType":"geometry","stylers":[{"lightness":100},{"visibility":"simplified"}]}] // TODO: replace [] by array from https://snazzymaps.com/
};

var map = new google.maps.Map(document.getElementById('map'), myOptions);
var marker = new google.maps.Marker({
  position: myLatlng,
  map: map,
  title: "You are here!"
});
</script>
